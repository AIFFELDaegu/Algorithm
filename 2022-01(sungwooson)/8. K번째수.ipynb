    "def solution(array, commands):\n",
    "    answer = []\n",
    "\n",
    "    for [i, j, k] in commands:\n",
    "        New = []\n",
    "        if i == j:\n",
    "            New.append(array[i-1])\n",
    "        else:\n",
    "            New = array[i-1:j]\n",
    "\n",
    "        New.sort()\n",
    "        answer.append(New[k-1])\n",
    "\n",
    "    return answer"
   ]
