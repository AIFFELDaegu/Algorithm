    "import re\n",
    "def solution(s):\n",
    "    number_chr= ['zero','one','two','three','four','five','six','seven','eight','nine']\n",
    "    answer=''\n",
    "    for i ,number in enumerate(number_chr) :\n",
    "        p = re.compile(number)\n",
    "        answer= p.sub(str(i),s)\n",
    "    \n",
    "    print(answer)\n",
    "    return answer"
